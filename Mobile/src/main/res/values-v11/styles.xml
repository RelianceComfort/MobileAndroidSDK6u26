<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
    <style name="EditTextBase.Normal.Table.TwoColumn.Special">
        <item name="android:background">@drawable/special_edit_text</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:textColorHint">@android:color/darker_gray</item>
        <item name="android:padding">2dp</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:layout_margin">1dp</item>
    </style>
    
    <!--
        Base application theme for API 11+. This theme completely replaces
        AppBaseTheme from res/values/styles.xml on API 11+ devices.
    -->
    <style name="CustomActionBarBaseTheme" parent="@style/Theme.AppCompat.Light">
        <!-- API 11 theme customizations can go here. -->
        <item name="android:actionBarStyle">@style/MyActionBar</item>
        <item name="android:actionOverflowButtonStyle">@style/CustomActionBarTheme.OverFlow</item>
    </style>
    
</resources>